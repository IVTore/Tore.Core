<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <PackageId>Tore.Core</PackageId>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <Authors>İhsan Volkan Töre</Authors>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Copyright>2021 İhsan Volkan Töre</Copyright>
    <Description>Sys: A static library of many utility functions.
Stl: A versatile string associated object list class.
Com: A Http Client for keeping things neat, tidy and simple.
</Description>
    <PackageTags>Tore Core Sys Stl Com Utilities Http Client</PackageTags>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/IVTore/Tore.Core</RepositoryUrl>
    <PackageReleaseNotes>* Corrected a minor bug in Com buildQuery method. 
This is the last version for Net 5.0.
Tore.Core 6.0.0 is for Net 6.0.</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <SignAssembly>false</SignAssembly>
    <ApplicationIcon />
    <Win32Resource />
    <Version>5.0.0</Version>
    <PackageProjectUrl>https://github.com/IVTore/Tore.Core</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>

</Project>
